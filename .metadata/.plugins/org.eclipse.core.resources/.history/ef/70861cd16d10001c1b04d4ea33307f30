package generics;

public class GenericPrinter<T extends Material> {//Material을 상속하는 클래스만 사용 가능
	private T material;
	
	public void setMaterial(T material) {
		this.material = material;
	}
	
	public T getMaterial() {
		return material;
	}
	
	public String toString() {
		return material.toString();
	}

}
